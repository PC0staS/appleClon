---
import Navbar from '../components/Navbar.tsx';
import Iphone17Pro from '../components/Iphone17Pro.astro';
import Layout from '../layouts/Layout.astro';
import IphoneAir from '../components/IphoneAir.astro';
import Iphone17 from '../components/Iphone17.astro';
import AirpodsGrid from '../components/AirpodsGrid.astro';
import Watch11 from '../components/Watch11.astro';
import IpadAir from '../components/IpadAir.astro';
import TradeIn from '../components/TradeIn.astro';
import WatchSE from '../components/WatchSE.astro';
import WatchUltra from '../components/WatchUltra.astro';
import Footer from '../components/Footer.astro';

