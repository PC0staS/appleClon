---

<Layout>
	<Navbar client:load />
	<Iphone17Pro />
	<IphoneAir />
	<Iphone17 />
	<div class="w-full h-auto grid lg:grid-cols-2 grid-cols-1 gap-4 mt-2">
		<AirpodsGrid />
		<Watch11 />
		<WatchSE />
		<WatchUltra />
		<IpadAir />
